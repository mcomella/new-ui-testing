package @ANDROID_PACKAGE_NAME@.tests;

abstract class UITestComponent {
    // UI test context associated with the component
    final private UITestContext mTestContext;

    public UITestComponent(UITestContext testContext) {
        mTestContext = testContext;
    }

    protected UITestContext getTestContext() {
        return mTestContext;
    }
}